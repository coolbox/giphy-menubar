{
  "name": "giphy-menubar",
  "version": "0.0.1",
  "description": "A minimal Giphy Menubar application",
  "main": "bootstrapper.js",
  "scripts": {
    "test": "mocha --require ./test/setup.js --recursive --compilers js:babel-register test/**/*.spec.js --require chai --reporter nyan",
    "test-watch": "npm test -- --watch",
    "lint": "eslint app test *.js",
    "build": "electron-packager . Example --platform=darwin --arch=x64 --version=0.36.0 --icon=Icon.icns",
    "start": "electron .",
    "watch": "watchify ./public/assets/js/index.js -t babelify -o public/assets/js/bundle.js --debug --verbose"
  },
  "author": "Pete Roome <pete.roome@gmail.com> (http://peteroome.com/)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coolbox/giphy-menubar.git"
  },
  "keywords": [
    "Giphy",
    "React"
  ],
  "bugs": {
    "url": "https://github.com/coolbox/giphy-menubar/issues"
  },
  "homepage": "https://github.com/coolbox/giphy-menubar#readme",
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babelify": "^7.3.0",
    "browser-sync": "^2.11.2",
    "browserify": "^12.0.1",
    "chai": "*",
    "electron-packager": "^5.1.0",
    "electron-prebuilt": "^0.36.0",
    "electron-reload": "^0.2.0",
    "enzyme": "^2.2.0",
    "eslint": "^3.4.0",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.2.1",
    "eslint-plugin-react": "^6.0.0",
    "exorcist": "^0.4.0",
    "gulp": "^3.9.1",
    "gulp-eslint": "^2.0.0",
    "gulp-if-else": "^1.0.3",
    "gulp-uglify": "^1.5.3",
    "jquery": "^2.2.3",
    "jsdom": "^9.2.1",
    "mocha": "*",
    "mockery": "^1.7.0",
    "react": "^15.3.1",
    "react-addons-test-utils": "^15.3.1",
    "react-dom": "^15.0.1",
    "react-router": "^2.2.2",
    "react-test-renderer": "^15.3.1",
    "sinon": "^1.17.5",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.13.0",
    "dotenv": "^2.0.0",
    "heap-api": "^1.0.1",
    "menubar": "*"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "testFileExtensions": [
      "js"
    ],
    "automock": false,
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react/",
      "<rootDir>/node_modules/react-dom/",
      "<rootDir>/node_modules/react-test-renderer/",
      "<rootDir>/node_modules/react-addons-test-utils/",
      "<rootDir>/node_modules/enzyme/"
    ]
  }
}
