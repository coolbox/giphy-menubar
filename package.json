{
  "private": true,
  "scripts": {
    "postinstall": "install-app-deps",
    "test": "mocha --require ./test/helpers/setup.js --recursive --compilers js:babel-register test/**/*.spec.js --require chai --reporter nyan",
    "test-watch": "npm test -- --watch",
    "lint": "eslint app test *.js",
    "start": "gulp start",
    "compile": "rimraf app/out && tsc",
    "build": "gulp build",
    "pack": "npm run compile && build --dir",
    "dist": "npm run compile && build",
    "release": "npm run compile && build",
    "watch": "watchify app/public/assets/js/index.js -t babelify -o app/public/assets/js/bundle.js --debug --verbose"
  },
  "build": {
    "appId": "org.develar.onshape",
    "category": "public.app-category.graphics-design",
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coolbox/giphy-menubar.git"
  },
  "keywords": [
    "Giphy",
    "React"
  ],
  "bugs": {
    "url": "https://github.com/coolbox/giphy-menubar/issues"
  },
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babelify": "^7.3.0",
    "browser-sync": "^2.11.2",
    "browserify": "^12.0.1",
    "chai": "*",
    "del": "^2.2.2",
    "electron": "^1.4.0",
    "electron-builder": "^7.9.0",
    "electron-packager": "^5.1.0",
    "electron-reload": "^1.0.2",
    "enzyme": "^2.2.0",
    "eslint": "^3.6.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "event-stream": "^3.3.4",
    "exorcist": "^0.4.0",
    "fs-jetpack": "^0.9.2",
    "gulp": "^3.9.1",
    "gulp-batch": "^1.0.5",
    "gulp-dotenv-to-json": "^1.1.2",
    "gulp-eslint": "^3.0.1",
    "gulp-if-else": "^1.0.3",
    "gulp-less": "^3.1.0",
    "gulp-merge-json": "^0.6.0",
    "gulp-plumber": "^1.1.0",
    "gulp-uglify": "^1.5.3",
    "gulp-util": "^3.0.7",
    "gulp-watch": "^4.3.9",
    "heap-api": "^1.0.1",
    "jquery": "^2.2.3",
    "jsdom": "^9.2.1",
    "menubar": "^5.1.0",
    "mocha": "*",
    "mockery": "^1.7.0",
    "react": "^15.3.1",
    "react-addons-test-utils": "^15.3.1",
    "react-dom": "^15.0.1",
    "react-masonry-component": "^4.2.1",
    "react-test-renderer": "^15.3.1",
    "rollup": "^0.36.0",
    "sinon": "^1.17.5",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.7.0"
  }
}
